{
  "name": "starter",
  "private": true,
  "description": "",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "develop": "GATSBY_ACTIVE_ENV=development gatsby clean && gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "GATSBY_ACTIVE_ENV=development gatsby clean && GATSBY_ACTIVE_ENV=development gatsby build && GATSBY_ACTIVE_ENV=development gatsby serve -H 0.0.0.0",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "gatsby": "^3.10",
    "gatsby-plugin-canonical-urls": "^3.10",
    "gatsby-plugin-react-helmet": "^4",
    "gatsby-plugin-sass": "^4.10",
    "gatsby-source-filesystem": "^3.10",
    "lodash": "^4.17.21",
    "node-sass": "^7.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "prettier": "^2.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshellington/gatsby-starter"
  },
  "bugs": {
    "url": "https://github.com/joshellington/gatsby-starter/issues"
  }
}
